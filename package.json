{
  "name": "with-tailwindcss-emotion",
  "version": "1.0.0",
  "scripts": {
    "dev": "node server.js",
    "export": "npm run build && next export",
    "build": "next build",
    "postbuild": "next-on-netlify",
    "start": "NODE_ENV=production nodemon server.js",
    "build:base-css": "tailwindcss build ./styles/tailwind.base.css -o ./styles/base.css",
    "format": "prettier --write \"**/*.{js,scss,css,html,json,md,less,tsx}\""
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^0.14.0",
    "@emotion/css": "^11.0.0-next.12",
    "@emotion/react": "^11.0.0-next.12",
    "@emotion/styled": "^11.0.0-next.12",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "cross-env": "^7.0.2",
    "cypress": "^4.10.0",
    "dotenv-webpack": "^2.0.0",
    "express": "^4.17.3",
    "global": "^4.4.0",
    "next": "latest",
    "next-on-netlify": "^2.3.0",
    "next-routes": "^1.4.2",
    "prettier": "^2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-typed": "^1.2.0",
    "reactstrap": "^8.5.1",
    "sass": "^1.26.10",
    "swr": "^0.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@emotion/babel-plugin": "^11.0.0-next.12",
    "@emotion/server": "^11.0.0-next.12",
    "@tailwindcss/ui": "^0.2.2",
    "@tailwindcssinjs/macro": "^0.3.1",
    "babel-plugin-macros": "2.8.0",
    "tailwindcss": "1.3.5"
  }
}
